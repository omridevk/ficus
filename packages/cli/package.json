{
  "name": "cli",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "build": "rollup -c",
    "prepublishOnly": "pnpm build"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs"
    },
    "./*": "./*",
    "./config": {
      "types": "./config.d.ts",
      "require": "./dist/config.cjs",
      "import": "./dist/config.mjs"
    }
  },
  "main": "./dist/cli.mjs",
  "module": "./dist/cli.mjs",
  "types": "./dist/cli.d.ts",
  "bin": {
    "ficus": "./ficus.mjs"
  },
  "dependencies": {
    "cac": "^6.7.12",
    "change-case": "^4.1.2",
    "debug": "^4.3.4",
    "fast-glob": "^3.2.11",
    "fs-extra": "^10.1.0",
    "mustache": "^4.2.0",
    "ora": "^6.1.2",
    "pathe": "^0.3.3",
    "prettier": "^2.7.1",
    "rimraf": "^3.0.2",
    "svgo": "^2.8.0",
    "vfile": "^5.3.4",
    "yargs": "^17.5.1"
  },
  "devDependencies": {
    "@rollup/plugin-alias": "^3.1.9",
    "@rollup/plugin-commonjs": "^22.0.2",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^13.3.0",
    "@rollup/plugin-typescript": "^8.3.4",
    "@types/debug": "^4.1.7",
    "@types/fs-extra": "^9.0.13",
    "@types/mustache": "^4.2.1",
    "@types/rimraf": "^3.0.2",
    "@types/svgo": "^2.6.3",
    "@types/yargs": "^17.0.11",
    "esbuild": "^0.14.54",
    "rollup": "^2.77.2",
    "rollup-plugin-esbuild": "^4.9.1",
    "typescript": "^4.6.4"
  }
}
